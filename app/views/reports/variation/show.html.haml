= content_for :header do
  = stylesheet_link_tag 'reports/variation/show', media: :all
  - @stanza.each do |stanza|
    = stanza.link

#nav-wrapper
  #navbar.navbar
    .navbar-inner
      %ul.nav
        - @stanza.each do |stanza|
          %li= link_to stanza.name, "##{stanza.name}"
%main
  .container
    %h1 Variation Report
    - @stanza.each do |stanza|
      %section{id: stanza.name}
        .page-header
          %h2= stanza.label
        = stanza.tag
